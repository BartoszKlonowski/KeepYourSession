{
  "name": "keep-your-session",
  "version": "0.1.0",
  "description": "Mozilla Firefox extension plugin to store and manage browsing sessions",
  "main": "manifest.json",
  "scripts": {
    "test": "jest",
    "build": "babel --config-file ./.babelrc ./app -Dd ./build && webpack --mode development",
    "install": "babel --config-file ./.babelrc ./app -Dd ./build && webpack --mode production"
