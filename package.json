  "bugs": {
    "url": "https://github.com/BartoszKlonowski/keep-your-session/issues"
  },
  "homepage": "https://github.com/BartoszKlonowski/keep-your-session#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/register": "^7.14.5",
    "eslint": "^7.28.0",
    "jest": "^27.0.5",
    "web-ext": "^6.1.0",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
