{
  "manifest_version": 2,
  "name": "KeepYourSession",
  "version": "0.1.0",

  "permissions": [
    "activeTab",
    "sessions",
    "tabs",
    "unlimitedStorage"
  ],

  "icons": {
    "830": "icons/KeepYourSessionIcon.png"
  },

  "description": "Let's user keep his sessions for future restoration",

  "browser_action": {
    "default_icon": "icons/KeepYourSessionIcon.png",
    "default_title": "KeepYourSession",
    "default_popup": "src/popup/MainPopup.html"
  }
}
