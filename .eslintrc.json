{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    },
    "plugins": [
        "prettier"
    ],
    "globals": {
        "browser": "writable",
        "window": "readonly",
        "test": "writable",
        "expect": "writable"
    }
}
